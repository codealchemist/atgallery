{
  "name": "atgallery",
  "version": "1.0.0",
  "title": "Automatic Twitter Gallery",
  "description": "Automatically generated image galleries for Twitter accounts.",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "build": "gulp build",
    "publish": "gulp build; git commit -m 'New build.'; git push; git push heroku master",
    "twitter-server": "node node_modules/twitter-server/src/index.js --config twitter-server.json",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "install-typings": "typings install",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
  },
  "keywords": [
    "twitter",
    "gallery",
    "automatic"
  ],
  "author": "Alberto Miranda <b3rt.js@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.*",
    "@angular/compiler": "2.*",
    "@angular/core": "2.*",
    "@angular/forms": "2.*",
    "@angular/http": "2.*",
    "@angular/platform-browser": "2.*",
    "@angular/platform-browser-dynamic": "2.*",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.*",
    "angular2-in-memory-web-api": "*",
    "angular2-infinite-scroll": "^0.2.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "gulp-useref": "^3.1.2",
    "install": "^0.8.1",
    "lightgallery": "git+https://github.com/codealchemist/lightGallery.git",
    "mongojs": "^2.4.0",
    "ng2-social-share": "git+https://github.com/codealchemist/ng2-social-share.git#alt",
    "npm": "^3.10.7",
    "reflect-metadata": "^0.1.3",
    "request": "^2.75.0",
    "requestify": "^0.2.3",
    "rxjs": "*",
    "systemjs": "0.19.27",
    "twitter-server": "^1.2.12",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "concurrently": "^2.2.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-if": "^2.0.1",
    "gulp-inject": "^4.1.0",
    "gulp-load-plugins": "^1.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^0.4.6",
    "gulp-tsc": "^1.2.3",
    "gulp-uglify": "^2.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "lite-server": "^2.2.0",
    "lodash": "^4.11.1",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.2",
    "tslint": "^3.7.4",
    "typescript": "^2.0.6",
    "typings": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codealchemist/atgallery.git"
  },
  "engines": {
    "node": "7.*"
  }
}
